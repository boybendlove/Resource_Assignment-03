push(data)\n            console.log(userMessage)\n            setLoad(true)\n            sessionStorage.setItem(`${id_user}`, JSON.stringify(userMessage))\n        }\n        }\n        postData()    \n   \n        setTextMessage('')\n\n    }\n\n    // Hàm này dùng để load dữ liệu message của user khi user gửi tin nhán\n    useEffect(() => {\n\n        if (load){\n\n            const fetchData = () => {\n\n                const mes = JSON.parse(sessionStorage.getItem(`${id_user}`))    \n                setMessage(mes)\n            }\n    \n            fetchData()\n\n            setLoad(false)\n\n        }\n        \n    }, [load])\n\n\n    // // Hàm này dùng để load dữ liệu message của user\n    // // Phụ thuộc state loadMessage\n    useEffect(() => {\n\n        const fetchData = () => {\n\n            const mes = JSON.parse(sessionStorage.getItem(`${id_user}`))    \n        setMessage(mes)\n\n        }\n\n        fetchData()\n        \n    }, [loadMessage])\n\n\n    useEffect(() => {\n\n        if (activeChat) {\n            console.log(\"On Chat\")\n        } else {\n            console.log(\"Off Chat\")\n        }\n\n    }, [activeChat])\n    \n\n    return (\n        <div className=\"wrapper_chat\">\n\n            <div className=\"chat_messenger\" onClick={onChat}>\n                <svg x=\"0\" y=\"0\" width=\"60px\" height=\"60px\"><g stroke=\"none\" strokeWidth=\"1\" fill=\"none\" fillRule=\"evenodd\">\n                    <g>\n                        <circle fill=\"#383838\" cx=\"30\" cy=\"30\" r=\"30\"></circle>\n                        <svg x=\"10\" y=\"10\">\n                            <g transform=\"translate(0.000000, -10.000000)\" fill=\"#FFFFFF\">\n                                <g id=\"logo\" transform=\"translate(0.000000, 10.000000)\">\n                                    <path d=\"M20,0 C31.2666,0 40,8.2528 40,19.4 C40,30.5472 31.2666,38.8 \n                                20,38.8 C17.9763,38.8 16.0348,38.5327 14.2106,38.0311 C13.856,37.9335 13.4